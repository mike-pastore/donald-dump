{
	"manifest_version": 2,
	"name": "Donald Dump",
	"version": "0.2",
	"description": "Turn Trump into Dump",
	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js": ["jquery-2.1.4.min.js","content.js"],
		"run_at": "document_end"
	}],
	"browser_action": {
		"default_icon": "trump48.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"48": "trump48.png",
		"128": "trump128.png"
	}
}